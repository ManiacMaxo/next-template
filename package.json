{
  "name": "some-next-app",
  "version": "0.1.0",
  "author": "John Doe",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "create:component": "node ./helper_scripts/commands/create_component.js",
    "create:page": "node ./helper_scripts/commands/create_page.js",
    "dev": "next",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "next": "^12.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.1",
    "handlebars": "^4.7.7",
    "husky": "^7.0.4",
    "inquirer": "^8.2.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "sass": "^1.43.4",
    "typescript": "^4.4.4"
  }
}
